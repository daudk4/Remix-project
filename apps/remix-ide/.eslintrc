{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "../../.eslintrc.json",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "standard/no-callback-literal": "off",
    "object-curly-spacing": "off"
  },
  "ignorePatterns": ["**/node_modules/**", "apps/remix-ide/src/assets/js/**/*.js"]
}
